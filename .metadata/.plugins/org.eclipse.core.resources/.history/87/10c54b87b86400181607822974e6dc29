package basics;

public class Employee {
	String firstname;
	String secondname;
	double monthlysalary;
	
	public String getFirstname() {
		return firstname;
	}
	public void setFirstname(String firstname) {
		this.firstname = firstname;
	}
	public String getSecondname() {
		return secondname;
	}
	public void setSecondname(String secondname) {
		this.secondname = secondname;
	}
	public double getMonthlysalary() {
		return monthlysalary;
	}
	public void setMonthlysalary(double monthlysalary) {
		this.monthlysalary = monthlysalary;
	}
	public static void main(String a[])
	{
		Employee e1=new Employee();
		Employee e2=new Employee();
		String f1=e1.getfirstname("Vinay");
		System.out.println(f1);
		String f2=e2.getfirstname("divya");
		System.out.println(f2);
		String s1=e1.getsecondname("uday");
		System.out.println(s1);
		String s2=e2.getsecondname("gowdru");
		double m1=e1.getmonthlysalary(20000.0);
		System.out.println(s2);
		double m2=e2.getmonthlysalary(25000.0);
		System.out.println(m1);
		
		
		
		
		
		System.out.println(m2);
	}	
	}

	
	
	
	

}
